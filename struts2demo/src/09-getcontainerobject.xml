<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>
	<package name="09-getcontainerobject" extends="struts-default" namespace="/09-getcontainerobject">
		<action name="login" class="com.freesoft.struts2.action.GetContainerObjectAction">
			<result name="success">/09-getcontainerobject/loginsucc.jsp</result>
			<result name="input">/09-getcontainerobject/login.jsp</result>
		</action>
		
		<action name="actioncontextlogin" class="com.freesoft.struts2.action.ActionContextLoginAction">
			<result name="success">/09-getcontainerobject/logintestsucc.jsp</result>
		</action>
		
		<action name="ioclogin" class="com.freesoft.struts2.action.IoCLoginAction">
			<result name="success">/09-getcontainerobject/logintestsucc.jsp</result>
		</action>
		
		
		<action name="servletactioncontextlogin" class="com.freesoft.struts2.action.ServletActionContextLoginAction">
			<result name="success">/09-getcontainerobject/logintestsucc.jsp</result>
		</action>
		
		
		<action name="servletioclogin" class="com.freesoft.struts2.action.ServletIoCLoginAction">
			<result name="success">/09-getcontainerobject/logintestsucc.jsp</result>
		</action>
	</package>
</struts>
