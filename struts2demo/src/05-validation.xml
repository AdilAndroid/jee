	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>
	<package name="05-validation" extends="struts-default" namespace="/05-validation">
		<action name="validationRegister" class="com.freesoft.struts2.action.ValidationRegisterAction">
			<result name="success">/05-validation/registersucc.jsp</result>
			<result name="input">/05-validation/register.jsp</result>
		</action>
		<action name="validationFrameworkRegister" class="com.freesoft.struts2.action.ValidationFrameworkRegisterAction">
			<result name="success">/05-validation/registersucc.jsp</result>
			<result name="input">/05-validation/register.jsp</result>
		</action>
	</package>
</struts>
